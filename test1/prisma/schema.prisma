// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model ProjectCard {
  id String @id @default(uuid()) // creates a unique id for each project
  name String
  description String
  imgUrl String
}
